{
  "Logging": {
    "IncludeScopes": "false",
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient.ITransport.LogicalHandler": "Warning"
    },
    "StatCollectablesInfo": {
      "Period": "30",
      "SaveToLocalOnly": "true"
    },
    "AllowedHosts": "*"
  }
}